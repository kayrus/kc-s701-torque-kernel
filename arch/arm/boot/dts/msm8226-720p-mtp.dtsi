&pm8226_mpps {
	mpp@a000 { /* MPP 1 */
		qcom,mode = <5>;
		qcom,pull = <1>;
		qcom,vin-sel = <3>;
		qcom,src-sel = <0>;
		qcom,master-en = <1>;
		qcom,aout-ref= <0>;
	};

	mpp@a100 { /* MPP 2 */
		qcom,mode = <6>;
		qcom,pull = <1>;
		qcom,vin-sel = <3>;
		qcom,src-sel = <0>;
		qcom,master-en = <1>;
		qcom,cs-out = <0>;
	};

	mpp@a200 { /* MPP 3 */
		qcom,mode = <5>;
		qcom,pull = <1>;
		qcom,vin-sel = <3>;
		qcom,src-sel = <0>;
		qcom,master-en = <1>;
		qcom,aout-ref= <0>;
	};

	mpp@a300 { /* MPP 4 */
		qcom,mode = <6>;
		qcom,pull = <1>;
		qcom,vin-sel = <3>;
		qcom,src-sel = <0>;
		qcom,master-en = <1>;
		qcom,cs-out = <0>;
	};

	mpp@a400 { /* MPP 5 */
		qcom,mode = <4>;
		qcom,pull = <1>;
		qcom,vin-sel = <3>;
		qcom,src-sel = <0>;
		qcom,master-en = <1>;
		qcom,ain-route = <0>;
	};

	mpp@a500 { /* MPP 6 */
		qcom,mode = <6>;
		qcom,pull = <1>;
		qcom,vin-sel = <3>;
		qcom,src-sel = <0>;
		qcom,master-en = <1>;
		qcom,cs-out = <0>;
	};

	mpp@a600 { /* MPP 7 */
		qcom,mode = <4>;
		qcom,pull = <1>;	/* The hardware ignores the configuration because operating fot this MPP is NOT bidirectional mode. */
		qcom,vin-sel = <3>;
		qcom,src-sel = <0>;
		qcom,master-en = <1>;
		qcom,ain-route = <2>;
	};

	mpp@a700 { /* MPP 8 */
		qcom,mode = <4>;
		qcom,pull = <1>;
		qcom,vin-sel = <3>;
		qcom,src-sel = <0>;
		qcom,master-en = <1>;
		qcom,ain-route = <3>;
	};
};

